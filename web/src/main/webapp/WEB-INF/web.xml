  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/dynPubError.jsp</location>
  </error-page>
  <security-constraint>
    <display-name>Application administration</display-name>
    <web-resource-collection>
      <web-resource-name>Protected Area</web-resource-name>
      <url-pattern>/admdp/index.jsp</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>TDPadministrator</role-name>
    </auth-constraint>
  </security-constraint>
  <login-config>
    <auth-method>BASIC</auth-method>
    <realm-name>TDP Administration</realm-name>
  </login-config>
  <security-role>
    <description>
      The role that is required to log in to the Administration pages
    </description>
    <role-name>TDPadministrator</role-name>
  </security-role>
  <context-param>
    <param-name>javax.servlet.jsp.jstl.fmt.localizationContext</param-name>
    <param-value>appStr</param-value>
  </context-param>
</web-app>